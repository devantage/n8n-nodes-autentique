{
  "name": "@devantage/n8n-nodes-autentique",
  "version": "1.1.0",
  "description": "n8n community nodes for Autentique's API",
  "contributors": [
    "Guilherme Nogueira <guilherme.nogueira@devantage.com.br>"
  ],
  "homepage": "https://github.com/devantage/n8n-nodes-autentique",
  "repository": {
    "type": "git",
    "url": "https://github.com/devantage/n8n-nodes-autentique.git"
  },
  "keywords": [
    "n8n-community-node-package",
    "autentique",
    "e-signature",
    "signature"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AutentiqueApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Autentique.node.js"
    ]
  },
  "scripts": {
    "clean": "rimraf dist/",
    "copy:assets": "copyfiles icons/icon.svg dist/ && copyfiles -u 1 src/**/*.gql dist/",
    "build:compile": "tsc",
    "build": "npm run clean && npm run build:compile && npm run copy:assets",
    "lint": "eslint --fix \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
